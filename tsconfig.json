{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "lib": ["ES2020"],
    "sourceRoot": "src/",
    "outDir": "out/"
  }
}
